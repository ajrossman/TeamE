    <div class="container" style="padding-top:50px;">
    	<div class="row">
  			<div class="col-md-5">
          <%= render "the_button" %>
  			</div>
  			<div class="col-md-7">
  				<div class="row streak-status">
            <div class="col-xs-10 col-md-10">
                <svg class="habit-bar" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                   width="605.058px" height="22.921px" viewBox="0 0 605.058 22.921" enable-background="new 0 0 605.058 22.921"
                   xml:space="preserve">
                <g>
                <% xspan = 29.107 %>
                <% ((Date.today - 20.days)..Date.today).each_with_index do |date, index| %>
                  <% if @eat_events.any? { |event| event.event_date == date } %>
                    <rect fill="#82C560" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% else %>
                    <rect fill="#D2E2C9" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% end %>
                <% end %>
                </g>
                </svg>
            </div>
            <div class="col-xs-1 col-md-1 streak-number"><%= @user.eat_streak %></div>
  					<div class="col-xs-1 col-md-1 streak-number">42</div>
  				</div>
  				
  				<div class="row streak-status">
            <div class="col-xs-10 col-md-10">
                <svg class="habit-bar" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                   width="605.058px" height="22.921px" viewBox="0 0 605.058 22.921" enable-background="new 0 0 605.058 22.921"
                   xml:space="preserve">
                <g>
                <% xspan = 29.107 %>
                <% ((Date.today - 20.days)..Date.today).each_with_index do |date, index| %>
                  <% if @exercise_events.any? { |event| event.event_date == date } %>
                    <rect fill="#82C560" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% else %>
                    <rect fill="#D2E2C9" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% end %>
                <% end %>
                </g>
                </svg>
            </div>
            <div class="col-xs-1 col-md-1 streak-number"><%= @user.exercise_streak %></div>
  					<div class="col-xs-1 col-md-1 streak-number">42</div>
  				</div>
  				
  				<div class="row streak-status">
            <div class="col-xs-10 col-md-10">
                <svg class="habit-bar" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                   width="605.058px" height="22.921px" viewBox="0 0 605.058 22.921" enable-background="new 0 0 605.058 22.921"
                   xml:space="preserve">
                <g>
                <% xspan = 29.107 %>
                <% ((Date.today - 20.days)..Date.today).each_with_index do |date, index| %>
                  <% if @outdoor_events.any? { |event| event.event_date == date } %>
                    <rect fill="#82C560" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% else %>
                    <rect fill="#D2E2C9" x="<%= xspan * index %>" width="22.921" height="22.921"/>
                  <% end %>
                <% end %>
                </g>
                </svg>
            </div>
            <div class="col-xs-1 col-md-1 streak-number"><%= @user.outdoor_streak %></div>
  					<div class="col-xs-1 col-md-1 streak-number">42</div>
  				</div>
  			</div>
		</div>
		
		
		
		<div class="row">
			<div class="col-xs-12">
				<div class="panel panel-default">
  					<div class="panel-heading">
    					<h3 class="panel-title">Today's to-do</h3>
  					</div>
  					<div class="panel-body">
    					Go to Ethan Allen Park in Burlington and climb to the top of the tower.
    					<div class="confirm-deny pull-right">
    					<button type="button" class="btn btn-success btn-lg">
  							<span class="glyphicon glyphicon-ok"></span>
						</button>
						<button type="button" class="btn btn-danger btn-lg">
  							<span class="glyphicon glyphicon-remove"></span>
						</button>
						</div>
  					</div>
				</div>
			</div>
		</div>
    </div> <!-- /container -->
